{
  "name": "nosaic-success-api",
  "description": "Nosaic Success API",
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "typecheck": "npm run typecheck --workspaces",
    "build": "npm run build --workspaces",
    "buildCore": "npm run build --workspace=@nosaic-success/core",
    "buildCloudflare": "npm run build --workspace=@nosaic-success/cloudflare",
    "deployCloud": "npm run deploy --workspace=@nosaic-success/cloudflare"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
